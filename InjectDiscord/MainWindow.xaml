<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:InjectDiscord"
        xmlns:Wpf="clr-namespace:Microsoft.Web.WebView2.Wpf;assembly=Microsoft.Web.WebView2.Wpf" x:Class="InjectDiscord.MainWindow"
        mc:Ignorable="d"
        Title="MainWindow" Height="712" Width="700" Background="#FF36393F">
    <Grid>
        <Button x:Name="Save" Content="Save" HorizontalAlignment="Left" Margin="10,615,0,0" VerticalAlignment="Top" Height="46" Width="131" Background="#FF5865F2" BorderBrush="{x:Null}" Foreground="White" FontFamily="Consolas" Click="Save_Click"/>
        <Button x:Name="sne" Content="Save And Exit" HorizontalAlignment="Left" Margin="537,616,0,0" VerticalAlignment="Top" Height="44" Width="131" Foreground="White" BorderBrush="{x:Null}" Background="#FF5865F2" FontFamily="Consolas" Click="sne_Click"/>
        <Wpf:WebView2 x:Name="browserCode" Margin="10,10,17,0" VerticalAlignment="Top" Height="600" Loaded="Window_Loaded" NavigationCompleted="NavigationCompleted"/>
    </Grid>
</Window>
